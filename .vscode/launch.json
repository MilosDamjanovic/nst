{
	"version": "0.2.0",
	"configurations": [

		{
			"type": "chrome",
			"request": "launch",
			"name": "Launch Chrome",
			"url": "http://localhost:4200",
			"webRoot": "${workspaceRoot}"
		},
		{
			"name": "Launch firefox",
			"type": "firefox",
			"request": "launch",
			"reAttach": true,
			"url": "http://localhost:4200",
			"webRoot": "${workspaceRoot}"
		},
		{
			"type": "chrome",
			"request": "attach",
			"name": "Attach to Chrome",
			"port": 9222,
			"webRoot": "${workspaceRoot}"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Debug e2e",
			"program": "${workspaceRoot}/node_modules/protractor/bin/protractor",
			"args": ["${workspaceRoot}/protractor.conf.js"]
		}
	]
}
